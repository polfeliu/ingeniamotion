{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["test-versions\\.json$"],
      "matchStrings": [
        "\"(?<depName>[^\"]+)\":\\s*{[^}]*\"currentVersion\":\\s*\"(?<currentVersion>[^\"]+)\"[^}]*\"datasource\":\\s*\"(?<datasource>[^\"]+)\"[^}]*\"depName\":\\s*\"(?<packageName>[^\"]+)\""
      ],
      "depNameTemplate": "{{{packageName}}}",
      "datasourceTemplate": "{{{datasource}}}"
    }
  ]
}
